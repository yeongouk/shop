package kr.kosmo.jobkorea.stats.dto;

import org.apache.ibatis.type.Alias;

@Alias("StatsDTO")
public class StatsDTO {
	private String pc_no;
	private String loginID;
	private String pcuseamt;
	private String foodamt;
	private String reg_date;
	
	
	public StatsDTO() {
		super();
	}
	
	
	public StatsDTO(String pc_no, String loginID, String pcuseamt, String foodamt, String reg_date) {
		super();
		this.pc_no = pc_no;
		this.loginID = loginID;
		this.pcuseamt = pcuseamt;
		this.foodamt = foodamt;
		this.reg_date = reg_date;
		
	
	}
	public String getPc_no() {
		return pc_no;
	}
	public void setPc_no(String pc_no) {
		this.pc_no = pc_no;
	}
	public String getLoginID() {
		return loginID;
	}
	public void setLoginID(String loginID) {
		this.loginID = loginID;
	}
	public String getPcuseamt() {
		return pcuseamt;
	}
	public void setPcuseamt(String pcuseamt) {
		this.pcuseamt = pcuseamt;
	}
	public String getFoodamt() {
		return foodamt;
	}
	public void setFoodamt(String foodamt) {
		this.foodamt = foodamt;
	}
	public String getReg_date() {
		return reg_date;
	}
	public void setReg_date(String reg_date) {
		this.reg_date = reg_date;
	}
	
	@Override
	public String toString() {
		return "StatsDTO [pc_no = " + pc_no + ", loginID=" + loginID + ", pcuseamt = " + pcuseamt + ", foodamt =" + foodamt + ", reg_date=" + reg_date + "]";
	}
	
}
